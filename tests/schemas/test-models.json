{
  "openapi": "3.0.0",
  "info": {
    "title": "Test API",
    "version": "1.0.0"
  },
  "paths": {
    "/api/v1/test-models": {
      "get": {
        "summary": "List test models",
        "operationId": "listTestModels",
        "responses": {
          "200": {
            "description": "List of test models",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": ["data"],
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/TestModel"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "TestModel": {
        "type": "object",
        "required": ["id", "name", "slug", "created_at", "updated_at"],
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64",
            "example": 1
          },
          "name": {
            "type": "string",
            "example": "Test Model Name"
          },
          "slug": {
            "type": "string",
            "example": "test-model-name"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "example": "2025-08-17T00:31:50.000000Z"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time",
            "example": "2025-08-17T00:31:50.000000Z"
          }
        }
      }
    }
  }
}